{
    "name": "pesky-ui",
    "description": "UI template for PeskyCMF and PeskyCMS",
    "version": "0.0.1",
    "keywords": [
        "css",
        "sass",
        "ui template",
        "admin ui"
    ],
    "homepage": "https://github.com/swayok/PeskyUI",
    "author": "Filippov Alexander",
    "scripts": {
        "css": "npm-run-all --sequential css-compile css-prefix css-minify css-copy",
        "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/pesky-ui.scss dist/css/pesky-ui.css",
        "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
        "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/pesky-ui.min.css dist/css/pesky-ui.css",
        "css-copy": "shx mkdir -p examples/dist/ && shx cp -r dist/css/ examples/dist/",
        "js": "npm-run-all --parallel js-compile-* --sequential js-minify js-copy",
        "js-compile": "npm-run-all --parallel js-compile-* --sequential js-copy",
        "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
        "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
        "js-minify": "npm-run-all --parallel js-minify-*",
        "js-minify-standalone": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/pesky-ui.js.map,includeSources,url=pesky-ui.min.js.map\" --output dist/js/pesky-ui.min.js dist/js/pesky-ui.js",
        "js-minify-bundle": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/pesky-ui.bundle.js.map,includeSources,url=pesky-ui.bundle.min.js.map\" --output dist/js/pesky-ui.bundle.min.js dist/js/pesky-ui.bundle.js",
        "js-copy": "shx mkdir -p examples/dist/ && shx cp -r dist/js/ examples/dist/",
        "dist": "npm-run-all --parallel css js",
        "watch": "npm-run-all --parallel watch-*",
        "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm run css\"",
        "watch-js": "nodemon --watch js/ --ext js --exec \"npm run js-compile\""
    },
    "style": "dist/css/pesky-ui.css",
    "sass": "scss/pesky-ui.scss",
    "main": "dist/js/pesky-ui",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swayok/PeskyUI.git"
    },
    "bugs": {
        "url": "https://github.com/swayok/PeskyUI/issues"
    },
    "license": "MIT",
    "dependencies": {},
    "peerDependencies": {
        "jquery": "1.9.1 - 3",
        "bootstrap": "^4.1.3"
    },
    "devDependencies": {
        "@babel/cli": "7.0.0-beta.52",
        "@babel/core": "7.0.0-beta.52",
        "@babel/preset-env": "7.0.0-beta.52",
        "autoprefixer": "^8.6.5",
        "babel-eslint": "^8.2.5",
        "babel-plugin-istanbul": "^4.1.6",
        "broken-link-checker": "^0.7.8",
        "clean-css-cli": "^4.1.11",
        "coveralls": "^3.0.2",
        "cross-env": "^5.2.0",
        "eslint": "^5.0.1",
        "eslint-plugin-compat": "^2.4.0",
        "glob": "^7.1.2",
        "htmllint-cli": "^0.0.7",
        "http-server": "^0.11.1",
        "jsunitsaucelabs": "^1.3.2",
        "karma": "^2.0.4",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.1",
        "karma-detect-browsers": "^2.3.2",
        "karma-firefox-launcher": "^1.1.0",
        "karma-qunit": "^2.1.0",
        "karma-sinon": "^1.0.5",
        "node-sass": "^4.9.1",
        "nodemon": "^1.17.5",
        "npm-run-all": "^4.1.3",
        "popper.js": "^1.14.3",
        "postcss-cli": "^5.0.1",
        "qunit": "^2.6.1",
        "rollup": "^0.62.0",
        "rollup-plugin-babel": "4.0.0-beta.5",
        "rollup-plugin-node-resolve": "^3.3.0",
        "shelljs": "^0.8.2",
        "shx": "^0.3.1",
        "sinon": "^6.1.2",
        "sri-toolbox": "^0.2.0",
        "stylelint": "^9.3.0",
        "stylelint-config-recommended-scss": "^3.2.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-order": "^0.8.1",
        "stylelint-scss": "^3.1.3",
        "uglify-js": "^3.4.3",
        "vnu-jar": "^18.3.0",
        "workbox-build": "^3.3.1"
    },
    "engines": {
        "node": ">=6"
    },
    "files": [
        "dist/",
        "js/",
        "scss/"
    ]
}
